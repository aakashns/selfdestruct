{
  "manifest_version": 2,
  "name": "SelfDestruct Inactive Tabs",
  "version": "0.0.3",
  "description": "Inactive tabs are closed automatically after 1 hour, unless you don't want them to be.",
  "browser_action": {
    "default_icon": "images/bomb.png"
  },
  "icons": {
    "128": "images/bomb.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "storage"
  ],
  "options_page": "options.html",
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  }
}