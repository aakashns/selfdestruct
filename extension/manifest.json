{
  "manifest_version": 2,
  "name": "SelfDestruct",
  "version": "0.0.1",
  "description": "Inactive tabs are closed automatically after 1 hour, unless you don't want them to be.",
  "browser_action": {
    "default_icon": "images/bomb.png"
  },
  "icons": {
    "128": "images/bomb.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs",
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}